{
    "server": {
        "host": "0.0.0.0",
        "port": 8000,
        "debug": false
    },
    "database": {
        "url": "sqlite:///./server_data.db",
        "echo": false
    },
    "ai": {
        "provider": "openai",
        "model": "gpt-4.1-nano",
        "temperature": 0.7,
        "max_tokens": 1000
    },    "api_keys": {
        "_comment": "API keys are now loaded from environment variables (.env file)",
        "openai": "",
        "anthropic": ""
    },
    "plugins": {
        "auto_load": true,
        "default_enabled": [
            "weather_module",
            "search_module"
        ]
    },
    "logging": {
        "level": "INFO",
        "file": "logs/server_{time:YYYY-MM-DD}.log"
    }
}